{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "Bundler",
    "jsx": "react-native",
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@lib/*": [
        "./lib/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@services/*": [
        "./services/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}